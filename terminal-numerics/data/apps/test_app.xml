<?xml version="1.0" encoding="UTF-8"?>
<app id="test_app">
	<name>Test App</name>
	<version>0.1</version>
	<description>Basic app that tests the expression parsing algorithms.</description>
	<standards>CCSS 3.NBT.A.1, CCSS 3.NF.A.1, ccss 3.nf.a.2.b</standards>
	<dependencies>app.test_app_3:1.10</dependencies>
	<layout type="border">
		<component pos="top" id="label" text="Graph interface for testing the expression resolver algorithms." />
		<component pos="center" id="graph" display="(x(-x^.2));x^2sqrt(-x);1/sin(x^.5);(x+2)(x-3);(x(-x^.2))/tan(x^.5)-(x+2)(x-3)" />
	</layout>
</app>